{
  "name": "react-native-nitro-sfsymbols",
  "version": "1.1.1",
  "description": "..",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "nitrogen",
    "nitro.json",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace react-native-nitro-sfsymbols-example",
    "test": "jest",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib",
    "prepare": "bob build",
    "nitrogen:codegen": "nitrogen",
    "nitrogen": "yarn nitrogen:codegen && node ./scripts/sync-nitro-config.cjs",
    "release": "release-it --only-version"
  },
  "keywords": [
    "react-native",
    "ios",
    "macos",
    "tvos",
    "visionos",
    "android",
    "sf-symbols",
    "icons",
    "nitro-modules"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mCodex/react-native-nitro-sfsymbols.git"
  },
  "author": "Mateus Andrade <mat.andrade@live.com> (https://github.com/mCodex)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mCodex/react-native-nitro-sfsymbols/issues"
  },
  "homepage": "https://github.com/mCodex/react-native-nitro-sfsymbols#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@evilmartians/lefthook": "^2.0.4",
    "@react-native/babel-preset": "0.82.1",
    "@react-native/eslint-config": "^0.82.1",
    "@release-it/conventional-changelog": "^10.0.2",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.7",
    "commitlint": "^20.1.0",
    "del-cli": "^7.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "nitrogen": "^0.31.10",
    "prettier": "^3.6.2",
    "react": "19.1.1",
    "react-native": "0.82.1",
    "react-native-builder-bob": "^0.40.16",
    "react-native-nitro-modules": "^0.31.10",
    "release-it": "^19.0.6",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-nitro-modules": "^0.31.10"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@3.6.1",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    }
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "custom",
        {
          "script": "nitrogen",
          "clean": "nitrogen/"
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "create-react-native-library": {
    "languages": "kotlin-swift",
    "type": "nitro-view",
    "version": "0.54.7"
  }
}
